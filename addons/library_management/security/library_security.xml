<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="group_library_librarian" model="res.groups">
        <field name="name">Bibliotecario</field>
        <field name="category_id" ref="base.module_category_library" />
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]" />
    </record>
    <record id="group_library_member" model="res.groups">
        <field name="name">Socio de Biblioteca</field>
        <field name="implied_ids" eval="[(4, ref('base.group_portal'))]" />
    </record>
    <record id="library_loan_rule_member" model="ir.rule">
        <field name="name">Socios: Ver solo sus pr√©stamos</field>
        <field name="model_id" ref="model_library_loan" />
        <field name="domain_force">[('member_id.partner_id', '=', user.partner_id.id)]</field>
        <field name="groups" eval="[(4, ref('group_library_member'))]" />
    </record>
</odoo>